package com.internousdev.library.action;

import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;

public class InfochangeAction extends ActionSupport implements SessionAware {

	public Map<String, Object> session;
	private String hoge1;
	public String hoge2;
	public String hoge3;
	public String selectmessage;

	public String execute() {
		System.out.println(hoge1);
		if (hoge1 != null) {
			selectmessage = "a";
			if (hoge2 != null) {
				selectmessage = "b";
				if (hoge3 != null) {
					selectmessage = "c";
				}
			}
		}

		return SUCCESS;
	}

	public String getHoge1() {
		return hoge1;
	}

	public void setHoge1(String hoge1) {
		this.hoge1 = hoge1;
	}

	public String getHoge2() {
		return hoge2;
	}

	public void setHoge2(String hoge2) {
		this.hoge2 = hoge2;
	}

	public String getHoge3() {
		return hoge3;
	}

	public void setHoge3(String hoge3) {
		this.hoge3 = hoge3;
	}

	@Override
	public void setSession(Map<String, Object> session) {
		this.session = session;
	}

}
