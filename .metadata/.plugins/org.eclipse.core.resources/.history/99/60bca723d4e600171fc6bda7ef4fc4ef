package com.internousdev.library.action;

import java.util.ArrayList;
import java.util.List;
import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;

public class InfochangeAction extends ActionSupport implements SessionAware {

	public Map<String, Object> session;

	List<String> hoge1 = new ArrayList<String>();
	List<String> hoge2 = new ArrayList<String>();
	List<String> hoge3 = new ArrayList<String>();
	public String selectmessage;

	public String execute() {
		System.out.println(hoge1);
		if (hoge1 != null) {
			selectmessage = "a";
			if (hoge2 != null) {
				selectmessage = "b";
				if (hoge3 != null) {
					selectmessage = "c";
				}
			}
		}

		return SUCCESS;
	}

	public List<String> getHoge1() {
		return hoge1;
	}

	public void setHoge1(List<String> hoge1) {
		this.hoge1 = hoge1;
	}

	public List<String> getHoge2() {
		return hoge2;
	}

	public void setHoge2(List<String> hoge2) {
		this.hoge2 = hoge2;
	}

	public List<String> getHoge3() {
		return hoge3;
	}

	public void setHoge3(List<String> hoge3) {
		this.hoge3 = hoge3;
	}

	@Override
	public void setSession(Map<String, Object> session) {
		this.session = session;
	}

}
