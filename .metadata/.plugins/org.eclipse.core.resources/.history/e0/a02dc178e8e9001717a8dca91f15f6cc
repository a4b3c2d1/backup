package com.internousdev.library.action;

import java.net.HttpURLConnection;
import java.net.URL;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;
import com.sun.xml.internal.ws.Closeable;

import sun.net.www.http.HttpClient;

public class BooksearchresultAction extends ActionSupport implements SessionAware {
	public String execute() {

		executeGet();

		return SUCCESS;
	}

}

	public static void executeGet() {

		URL url=new URL("https://www.googleapis.com/books/v1/volumes?q=ISBN=%E5%A4%8F%E7%9B%AE%E6%BC%B1%E7%9F%B3");

		HttpURLConnection connection = null;

		try{
		connection= (HttpURLConnection) url.openConnection();
		connection.setRequestMethod("GET");
	}


}
