<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
	<!-- 対象のpackageを設定 -->
	<package name="com.internousdev.library.action" extends="struts-default">


		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.library.action.HomeAction"
			method="execute">
			<result name="success">login.jsp</result>
		</action>

		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.library.action.LoginAction"
			method="execute">
			<result name="success">mainpage.jsp</result>
			<result name="error">loginerror.jsp</result>
		</action>

		<!-- UserinfoAction -->
		<action name="UserinfoAction" class="com.internousdev.library.action.UserinfoAction"
			method="execute">
			<result name="success">info.jsp</result>
		</action>



		<!-- InfochangeAction -->
		<action name="InfochangeAction" class="com.internousdev.library.action.InfochangeAction"
			method="aaa">
			<result name="success">infochange.jsp</result>
		</action>

		<action name="InfochangeAction" class="com.internousdev.library.action.InfochangeAction"
			method="bbb">
			<result name="success">infochange.jsp</result>
		</action>

		<action name="InfochangeAction" class="com.internousdev.library.action.InfochangeAction"
			method="ccc">
			<result name="success">infochange.jsp</result>
		</action>



		<!-- InfochangeConfirmAction -->
		<action name="InfochangeConfirmAction"
			class="com.internousdev.library.action.InfochangeConfirmAction"
			method="aaa">
			<result name="success">infochangeConfirm.jsp</result>
			<result name="error">infochange.jsp</result>
		</action>

		<action name="InfochangeConfirmAction"
			class="com.internousdev.library.action.InfochangeConfirmAction"
			method="bbb">
			<result name="success">infochangeConfirm.jsp</result>
			<result name="error">infochange.jsp</result>
		</action>

		<action name="InfochangeConfirmAction"
			class="com.internousdev.library.action.InfochangeConfirmAction"
			method="ccc">
			<result name="success">infochangeConfirm.jsp</result>
			<result name="error">infochange.jsp</result>
		</action>



		<!-- InfochangeConpleteAction -->
		<action name="InfochangeConpleteAction"
			class="com.internousdev.library.action.InfochangeConpleteAction"
			method="aaa">
			<result name="success">infochangeConplete.jsp</result>
		</action>

		<action name="InfochangeConpleteAction"
			class="com.internousdev.library.action.InfochangeConpleteAction"
			method="bbb">
			<result name="success">infochangeConplete.jsp</result>
		</action>

		<action name="InfochangeConpleteAction"
			class="com.internousdev.library.action.InfochangeConpleteAction"
			method="ccc">
			<result name="success">infochangeConplete.jsp</result>
		</action>



		<!-- MainpagebackAction -->
		<action name="MainpagebackAction"
			class="com.internousdev.library.action.MainpagebackAction" method="execute">
			<result name="success">mainpage.jsp</result>
		</action>


		<!--CreatemanagerAction -->
		<action name="CreatemanagerAction"
			class="com.internousdev.library.action.CreatemanagerAction" method="execute">
			<result name="success">createmanager.jsp</result>
		</action>


		<!-- CreatemanagerConfirmAction -->
		<action name="CreatemanagerConfirmAction"
			class="com.internousdev.library.action.CreatemanagerConfirmAction"
			method="execute">
			<result name="success">createmanagerConfirm.jsp</result>
			<result name="error">createmanager.jsp</result>
		</action>

		<!-- CreatemanagerConpleteAction -->
		<action name="CreatemanagerConpleteAction"
			class="com.internousdev.library.action.CreatemanagerConpleteAction"
			method="execute">
			<result name="success">createmanagerConplete.jsp</result>
		</action>

		<!-- LogoutAction -->
		<action name="LogoutAction" class="com.internousdev.library.action.LogoutAction"
			method="execute">
			<result name="success">login.jsp</result>
		</action>


		<!-- BookpageAction -->
		<action name="BookpageAction" class="com.internousdev.library.action.BookpageAction"
			method="execute">
			<result name="success">bookpage.jsp</result>
		</action>


		<!-- UserAction -->
		<action name="UserAction" class="com.internousdev.library.action.UserAction"
			method="execute">
			<result name="success">user.jsp</result>
		</action>


		<!-- UsercreateAction -->
		<action name="UsercreateAction" class="com.internousdev.library.action.UsercreateAction"
			method="execute">
			<result name="success">usercreate.jsp</result>
		</action>

		<!-- UsercreateConfirmAction -->
		<action name="UsercreateConfirmAction"
			class="com.internousdev.library.action.UsercreateConfirmAction"
			method="execute">
			<result name="success">usercreateconfirm.jsp</result>
		</action>
	</package>
</struts>